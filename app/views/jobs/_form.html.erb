<h3>Add Job</h3>
	<%= form_for([@car, @car.jobs.build]) do |f| %>
	<p>
		<%= f.label :title %><br>
		<%= f.text_field(:title, {:class => 'form-control'}) %>
		<%= f.label :date %><br>
		<%= f.text_field(:title, {:class => 'form-control'}) %>
		<%= f.label :cost %><br>
		<%= f.text_field(:title, {:class => 'form-control'}) %>
	</p>
	<p>
		<%= f.label :text %><br>
		<%= f.text_area(:text, {:class => 'form-control'}) %>
	</p>
	<p>
		<%= f.submit({:class => 'btn btn-primary'}) %>
	</p>
<% end %>